{
  "name": "vuedl",
  "version": "0.1.2",
  "description": "Vue dialog helper",
  "scripts": {
    "build2": "npm run build && cp -r dist/ ../../cmpact/billing-front/node_modules/vuedl/dist/ && cp -r src/ ../../cmpact/billing-front/node_modules/vuedl/src/ && cp -r dist/ ../vuetify-dialog/node_modules/vuedl/dist/ && cp -r src/ ../vuetify-dialog/node_modules/vuedl/src/",
    "build": "npm run lint && npm run build:module && npm run build:browser",
    "build:module": "bili --name vuedl --format cjs,es --plugin vue --js buble",
    "build:browser": "bili --name vuedl --format umd,umd-min --external vue --global.vue \"Vue\" --plugin vue --js buble",
    "lint": "eslint --ext js --ext vue src",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build",
    "release": "bash build/release.sh",
    "test": "jest --env=jsdom ",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls",
    "test1": "node_modules/.bin/jest --env=jsdom --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "main": "dist/vuedl.cjs.js",
  "module": "dist/vuedl.esm.js",
  "browser": "dist/vuedl.js",
  "cdn": "dist/vuedl.min.js",
  "unpkg": "dist/vuedl.min.js",
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yariksav/vuedl.git"
  },
  "keywords": [
    "vue",
    "dialog",
    "modal"
  ],
  "author": "Yaroslav Savaryn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yariksav/vuedl/issues"
  },
  "homepage": "https://github.com/yariksav/vuedl#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "bili": "^3.4.2",
    "coveralls": "^3.0.2",
    "debug": "^4.1.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "merge": ">=1.2.1",
    "rollup-plugin-vue": "^4.3.2",
    "vue": "^2.5.21",
    "vue-jest": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "peerDependencies": {
    "vue": "*"
  },
  "eslintIgnore": [
    "dist/"
  ]
}
